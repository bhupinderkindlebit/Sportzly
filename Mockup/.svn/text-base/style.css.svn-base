body {

	margin:0;

	padding:0;

	font-size:12px;

	font-family:Arial, Helvetica, sans-serif;

	background:url(images/bg-body.png) top left repeat;

}

p, h1, h2, h3, h4, h5, h6 {

	margin:0;

	padding:0;

}

img {

	border:none;

}

a {

	text-decoration:none;

}

ul {

	margin:0;

	padding:0;

	list-style:none;

	height:59px;

	line-height:59px;

}

li {	

	float:left;

}

.clear {

	clear:both;

}

 



/*top-body*/

header {

    background: url("images/bg-top-body.png") repeat-x scroll left top transparent;

    box-shadow: 0 20px 50px #CCCCCC;

    height: 59px;

    width: 100%;

}

header section.wrapper-top-body {

	width:980px;

	margin:0 auto;

}

header section.wrapper-top-body nav ul {

	float:left;

}

