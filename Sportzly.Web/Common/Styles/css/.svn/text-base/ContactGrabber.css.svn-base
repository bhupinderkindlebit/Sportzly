    .contacts-tpl .tpl-footer .tpl-footer-wrapper{background: white;border-radius: 5px;padding: 10px;margin: 10px;width: 550px;margin-left: 12px;height: 25px;}
    .contacts-tpl .tpl-footer .tpl-footer-left{width: 80px;margin-bottom: 12px;}
