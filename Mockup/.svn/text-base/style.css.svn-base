    text-align: center;

    width: 69px;

	background:url(images/boder-li.png) top left no-repeat;

}

header section.wrapper-top-body nav ul li a {

    color: #666666;

    font-weight: bold;

}

.logo {

    float: left;   

    margin-right: 74px;

    margin-top: 10px;

}

header section.wrapper-top-body .search {

    background: url("images/bg-search.png") repeat-x scroll left top transparent;

    border-radius: 5px 5px 5px 5px;

    float: left;

    height: 27px;

    margin-right: 10px;

    margin-top: 15px;

    position: relative;

    width: 190px;

}

header section.wrapper-top-body .search form input {

    background: none repeat scroll 0 0 transparent;

   

    float: left;

    height: 27px;

    line-height: 27px;

    padding-left: 10px;

	

}

header section.wrapper-top-body .search input.icon-search  {

     height: 17px;

    position: absolute;

    right: 12px;

    top: 8px;

