    .contacts-tpl .tpl-login-wrapper .textclass{width:200px}
    .contacts-tpl .tpl-contacts{height:280px;overflow:hidden;overflow-x:hidden;margin-top: 12px;color:Black;}
    .contacts-tpl .tpl-contacts ul{margin:0px;padding:0px}
    .contacts-tpl .tpl-contacts ul li{float: left;list-style-type: none;background-color: white;border-radius: 4px;border:3px solid #E4DFDF;margin-bottom: 3px;margin-left: 3px;}
    .contacts-tpl .tpl-contacts-li-hover{border:3px solid #97C379 !important;cursor:pointer;}
    .contacts-tpl .tpl-contacts-li-select{border:3px solid #97C379 !important;cursor:pointer;}
    .contacts-tpl .tpl-contacts-li-select-iconholder{height:0px;width:0px;}
    .contacts-tpl .tpl-contacts-li-select-icon{height:16px;width:16px;background-color:#97C379;background-image:url(../../../images/select.png);background-repeat: no-repeat;background-position: 2px;}
    .contacts-tpl .tpl-contacts-wrapper{height:250px;padding: 10px;float: left;padding-right: 4px;margin-left: 12px;width: 555px;border-radius: 5px;background-color: White;padding-left: 11px;}
    .contacts-tpl .tpl-contacts-list{float:left}
    .contacts-tpl .tpl-contacts-holder{width:180px}
    .contacts-tpl .tpl-contacts-holder img{width: 50px;height: 50px;}
    .contacts-tpl .tpl-header-icon img{width:60px}
    .contacts-tpl .tpl-header-icon{border: 1px solid #6D84B4;padding: 5px;margin-top: -3px;padding-bottom: 3px;margin-left: -8px;background-color: white;border-radius: 2px;}
    .contacts-tpl .tpl-header-icon:hover{cursor:pointer;}
    .contacts-tpl .tpl-service-list{display:none;}
    .contacts-tpl .tpl-service-list .service-list{z-index:9;background-color: white;border: 3px solid #6D84B4;border-top:0px;top: 21px;border-bottom-left-radius: 5px;border-bottom-right-radius: 5px;height: 120px;overflow-x: hidden;overflow-y: scroll;left: -10px;width: 102px;text-align: center;}
    .contacts-tpl .tpl-service-list img{cursor:pointer;width:60px}
    .contacts-tpl .tpl-service-list ul{margin:0px;padding:0px}
    .contacts-tpl .service-holder{width:92px}
    .contacts-tpl .tpl-service-list ul li{cursor:pointer;list-style-type: none;background-color: white;margin-bottom: 3px;margin-left: 3px;margin-top: 3px;width: 80px;border-bottom: 1px solid #F1EBEB;line-height: 20px;}
    .contacts-tpl .clear-contacts{cursor:pointer;}
    .contacts-tpl .tpl-contacts-holder .tpl-right{width: 112px;height: 46px;text-overflow: hidden;padding:4px}
    .contacts-tpl .tpl-contacts-holder .tpl-left{padding:4px}
