<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="MyUserDetailViewControl.ascx.cs"
    Inherits="Sportzly.Web.Controls.MyUserDetailViewControl" %>
<div class="user leftPanelUserDetails">
    <a class="user-img" data-bind="attr: { href: 'profile.aspx?' + EmailId() }">
        <img alt="" data-bind="attr: { src: ProfileFoto }"></a>
    <div class="user-details" style="margin-bottom: 0px;">
        <a data-bind="attr: { href: 'profile.aspx?' + EmailId() }" style="background: none;
            color: #282828; float: none;">
            <h3 class="username" data-bind="text: DisplayName">
            </h3>
        </a>
    </div>
</div>
