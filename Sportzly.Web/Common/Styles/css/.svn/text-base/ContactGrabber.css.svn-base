    .contacts-tpl .tpl-noresultsfound{background-image: url(../../../images/noresultsfound.png);background-repeat: no-repeat;background-position: center;}
    .contacts-tpl .tpl-nofriendsselected{background-image: url(../../../images/nofriendsselected.png);background-repeat: no-repeat;background-position: center;}
    .contacts-tpl .searchTxt{height:12px;font-family: 'lucida grande',tahoma,verdana,arial,sans-serif;font-size: 11px;width:200px;}
    .contacts-tpl .selected-contacts,.all-contacts{cursor:pointer}
    .contacts-tpl .textclass{padding: 3px;color: #000000;border-top: solid 1px #F1F1F1;border-left: solid 1px #F1F1F1;border-right: solid 1px #DFDFDF;border-bottom: solid 1px #cccccc;background: url(http://stescodes.com/images/inputbg.gif) top left no-repeat;}
    .contacts-tpl .subjectTxt{width:350px}
    .contacts-tpl .sendBtn{background-color: #dd4b39;color: White;cursor: pointer;padding: 5px;padding-left: 20px;padding-right: 20px;border-radius: 2px;margin-top:1px}
    .contacts-tpl .ac_results{position: absolute;width: 150px;top: 42px;left: 106px;background-color: white;padding: 4px;font-family: 'lucida grande',tahoma,verdana,arial,sans-serif;font-size: 11px;color:Black}
    .contacts-tpl .ac_results ul{margin:0px;padding:0px}
    .contacts-tpl .ac_results ul li{list-style-type:none;text-align:left}
    .contacts-tpl .ac_even,.ac_odd{border-bottom: 1px solid #f0f0f7;padding-top: 3px;padding-bottom: 3px;cursor:pointer;}
    .contacts-tpl .ac_over{background-color:#f0f0f7;color:Black important;}
    .contacts-tpl .hdrAlign{display: table-cell;line-height: 20px;}
    .contacts-tpl .tpl-login-bar{display:none;margin-top: 15px;background-color: white;width: 554px;margin-left: 12px;padding: 5px;border-radius: 5px;padding-left: 10px;margin-bottom: -4px;cursor: pointer;}
    .contacts-tpl .tpl-expand{background-image:url(../../../images/sortasc.png);background-repeat:no-repeat}
    .contacts-tpl .tpl-collapse{background-image:url(../../../images/sortleft.png);background-repeat:no-repeat;background-position: 10px 7px;padding-left: 35px;width: 529px !important;}
    .contacts-tpl .tpl-grab-contacts{cursor:pointer}
    .contacts-tpl .loading-content{padding-left: 5px;}
    .contacts-tpl .tpl-error-message{display:none;color:Red;line-height: 36px;display: table-cell;margin-left: 10px;width: 300px;}
    .contacts-tpl .tpl-login-outlook{background-image:url(../../../images/sload.gif);background-repeat:no-repeat;background-position:center;}
    .contacts-tpl .tpl-login-outlook{display:none;margin-top: 15px;height: 100px;background-color: white;width: 550px;margin-left: 12px;padding: 10px;border-radius: 5px;}
    .contacts-tpl .service-icon-hover {border-left: 1px solid white;}
    .contacts-tpl .service-icon{background-image:url(../../../images/sortasc.png);background-repeat:no-repeat;height:18px;width:16px;margin-top: -3px;background-position: 2px;}
    #popupcontainer .closeboxholder{position:relative;width:642px}
    #popupcontainer .closebox{position:absolute;top:-10px;right:35px;z-index:99}
    .contacts-tpl .tpl-ad{height:15px;width: 100% !important;}
    .contacts-tpl .powered{margin-top: -5px;margin-right: 15px;color: #A8A4A4;text-shadow: 1px 1px #FFF9F9;}
    .contacts-tpl .powered a{color: #A8A4A4;text-shadow: 1px 1px #FFF9F9;}
        
