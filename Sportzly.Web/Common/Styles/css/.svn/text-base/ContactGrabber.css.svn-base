    .contacts-tpl{background-image:url('../../../images/customnamebg.png');background-repeat:repeat;border: 1px solid #C2BDBD;}
    .contacts-tpl{min-height: 130px;width:595px;border-radius:5px;font-family: 'lucida grande',tahoma,verdana,arial,sans-serif;font-size: 11px;padding-bottom: 0px;}
    .contacts-tpl .tpl-header{height:30px;color:White;margin-top: 4px;}
    .contacts-tpl .tpl-header .tpl-header-wrapper{border-radius: 3px;margin-left: 10px;width:555px;background-color: #6D84B4;height: 25px;padding-top: 5px;padding-left: 10px;padding-right: 10px;}
